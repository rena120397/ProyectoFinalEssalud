<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletMantenimiento</servlet-name>
        <servlet-class>controlador.ServletMantenimiento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInsertar</servlet-name>
        <servlet-class>controlador.ServletInsertar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletListar</servlet-name>
        <servlet-class>controlador.ServletListar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletIntranet</servlet-name>
        <servlet-class>controlador.ServletIntranet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletControler</servlet-name>
        <servlet-class>controlador.ServletControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pdf</servlet-name>
        <servlet-class>controlador.pdf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletMantenimiento</servlet-name>
        <url-pattern>/Mantenimiento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInsertar</servlet-name>
        <url-pattern>/Insertar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletListar</servlet-name>
        <url-pattern>/Listar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletIntranet</servlet-name>
        <url-pattern>/Intranet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletControler</servlet-name>
        <url-pattern>/Controler</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>servletsalud</servlet-name>
        <servlet-class>controlador.servletsalud</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletValidacion</servlet-name>
        <servlet-class>controlador.ServletValidacion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletsalud</servlet-name>
        <url-pattern>/servletsalud</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletValidacion</servlet-name>
        <url-pattern>/ServletValidacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pdf</servlet-name>
        <url-pattern>/pdf</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
